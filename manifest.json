{
    "manifest_version": 2,
    "name": "DarkMod",
    "version": "1.1",
    "description": "Simple soundcloud darkmod",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://soundcloud.com/*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}